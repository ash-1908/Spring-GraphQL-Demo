input ProductInput {
    stock: Int!
    category: String!
    name: String!
}

type ProductCatalog {
    id: ID
    stock: Int
    category: String
    name: String
}

type Query {
    getAllProducts: [ProductCatalog]
    getProduct(id: Int!) : ProductCatalog
}

type Mutation {
    addNewStock(id: Int!, stock: Int!) : ProductCatalog
    addNewProduct(product: ProductInput!) : ProductCatalog
}
